<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TranslucentDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="colorPrimaryDark">@color/primary_dark_material_light</item>
        <item name="colorPrimary">@color/primary_material_light</item>
        <item name="colorAccent">@color/accent_material_light</item>
        <!--<item name="colorAccent">?colorAccent</item>-->
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="cusToolbarNavigationButtonStyle" parent="@style/Widget.AppCompat.Toolbar.Button.Navigation">
        <item name="android:minWidth">0dp</item>  <!--default 56dp-->
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
    </style>

    <style name="cusToolbarStyle" parent="@style/Widget.AppCompat.Toolbar">
        <item name="titleMargin">0dp</item> <!--default 4dp-->
        <item name="contentInsetStartWithNavigation">0dp</item> <!--default @dimen/abc_action_bar_content_inset_with_nav-->
    </style>
</resources>